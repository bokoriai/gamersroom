<div class="container">
    <div class="w-50">
        <h2 class="heading">掲示板新規作成</h2>
            <br>
            <%= form_with model: @board, url: public_boards_path, method: :post, local: true do |f| %>
            <%= f.label :title, "作成掲示板タイトル" %>
            <br>
            <%= f.text_field :title %>
            <br>
            <%= f.submit '掲示板作成', class: 'btn btn-primary' %>
            <% end %>
    </div>
</div>